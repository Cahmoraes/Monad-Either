    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "2.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.2.2",
    "vitest": "^2.1.4"
  }
